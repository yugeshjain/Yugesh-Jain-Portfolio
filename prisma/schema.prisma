generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("PLANETSCALE_PRISMA_DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  relationMode = "prisma"
}

model likes {
  slug  String @id @db.VarChar(128)
  count BigInt @default(1)
}
